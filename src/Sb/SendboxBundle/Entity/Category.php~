<?php

namespace Sp\SpotoraBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Sp\SpotoraBundle\Entity\Category
 *
 * @ORM\Table()
 * @ORM\Entity
 */
class Category
{

    /**
     * @var integer $id
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @ORM\OneToMany(targetEntity="Announce", mappedBy="category", cascade={"remove", "persist"})
     */
    protected $announces;
    
    /**
     * @var integer $root_id
     *
     * @ORM\Column(name="root_id", type="integer")
     */
    private $root_id;
    
    /**
     * @var integer $ordering
     *
     * @ORM\Column(name="ordering", type="integer")
     */
    private $ordering;

    /**
     * @var string $title
     *
     * @ORM\Column(name="title", type="string", length=255)
     */
    private $title;

    /**
     * @var text $description
     *
     * @ORM\Column(name="description", type="text", nullable=true)
     */
    private $description;
    
    /**
     * @var text $priority
     *
     * @ORM\Column(name="priority", type="integer")
     */
    private $priority;
    
    /**
     * @var text $status
     *
     * @ORM\Column(name="status", type="integer")
     */
    private $status;
    
    /**
     * To String
     *
     * @return string
     */
    public function __toString()
    {
        return $this->title;
    }
    
    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set category_id
     *
     * @param integer $categoryId
     */
    public function setCategoryId($categoryId)
    {
        $this->category_id = $categoryId;
    }

    /**
     * Get category_id
     *
     * @return integer 
     */
    public function getCategoryId()
    {
        return $this->category_id;
    }

    /**
     * Set title
     *
     * @param string $title
     */
    public function setTitle($title)
    {
        $this->title = $title;
    }

    /**
     * Get title
     *
     * @return string 
     */
    public function getTitle()
    {
        return $this->title;
    }

    /**
     * Set description
     *
     * @param text $description
     */
    public function setDescription($description)
    {
        $this->description = $description;
    }

    /**
     * Get description
     *
     * @return text 
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set city_id
     *
     * @param integer $cityId
     */
    public function setCityId($cityId)
    {
        $this->city_id = $cityId;
    }

    /**
     * Get city_id
     *
     * @return integer 
     */
    public function getCityId()
    {
        return $this->city_id;
    }

    /**
     * Set department_id
     *
     * @param integer $departmentId
     */
    public function setDepartmentId($departmentId)
    {
        $this->department_id = $departmentId;
    }

    /**
     * Get department_id
     *
     * @return integer 
     */
    public function getDepartmentId()
    {
        return $this->department_id;
    }

    /**
     * Set zip_id
     *
     * @param integer $zipId
     */
    public function setZipId($zipId)
    {
        $this->zip_id = $zipId;
    }

    /**
     * Get zip_id
     *
     * @return integer 
     */
    public function getZipId()
    {
        return $this->zip_id;
    }

    /**
     * Set price
     *
     * @param integer $price
     */
    public function setPrice($price)
    {
        $this->price = $price;
    }

    /**
     * Get price
     *
     * @return integer 
     */
    public function getPrice()
    {
        return $this->price;
    }

    /**
     * Set phone
     *
     * @param string $phone
     */
    public function setPhone($phone)
    {
        $this->phone = $phone;
    }

    /**
     * Get phone
     *
     * @return string 
     */
    public function getPhone()
    {
        return $this->phone;
    }

    /**
     * Set email
     *
     * @param string $email
     */
    public function setEmail($email)
    {
        $this->email = $email;
    }

    /**
     * Get email
     *
     * @return string 
     */
    public function getEmail()
    {
        return $this->email;
    }

    /**
     * Set tags
     *
     * @param string $tags
     */
    public function setTags($tags)
    {
        $this->tags = $tags;
    }

    /**
     * Get tags
     *
     * @return string 
     */
    public function getTags()
    {
        return $this->tags;
    }

    /**
     * Set kind
     *
     * @param boolean $kind
     */
    public function setKind($kind)
    {
        $this->kind = $kind;
    }

    /**
     * Get kind
     *
     * @return boolean 
     */
    public function getKind()
    {
        return $this->kind;
    }

    /**
     * Set view
     *
     * @param integer $view
     */
    public function setView($view)
    {
        $this->view = $view;
    }

    /**
     * Get view
     *
     * @return integer 
     */
    public function getView()
    {
        return $this->view;
    }

    /**
     * Set popular
     *
     * @param integer $popular
     */
    public function setPopular($popular)
    {
        $this->popular = $popular;
    }

    /**
     * Get popular
     *
     * @return integer 
     */
    public function getPopular()
    {
        return $this->popular;
    }

    /**
     * Set is_published
     *
     * @param boolean $isPublished
     */
    public function setIsPublished($isPublished)
    {
        $this->is_published = $isPublished;
    }

    /**
     * Get is_published
     *
     * @return boolean 
     */
    public function getIsPublished()
    {
        return $this->is_published;
    }

    /**
     * Set is_moderated
     *
     * @param boolean $isModerated
     */
    public function setIsModerated($isModerated)
    {
        $this->is_moderated = $isModerated;
    }

    /**
     * Get is_moderated
     *
     * @return boolean 
     */
    public function getIsModerated()
    {
        return $this->is_moderated;
    }

    /**
     * Set is_private
     *
     * @param boolean $isPrivate
     */
    public function setIsPrivate($isPrivate)
    {
        $this->is_private = $isPrivate;
    }

    /**
     * Get is_private
     *
     * @return boolean 
     */
    public function getIsPrivate()
    {
        return $this->is_private;
    }

    /**
     * Set is_spam
     *
     * @param boolean $isSpam
     */
    public function setIsSpam($isSpam)
    {
        $this->is_spam = $isSpam;
    }

    /**
     * Get is_spam
     *
     * @return boolean 
     */
    public function getIsSpam()
    {
        return $this->is_spam;
    }

    /**
     * Set finished_at
     *
     * @param datetime $finishedAt
     */
    public function setFinishedAt($finishedAt)
    {
        $this->finished_at = $finishedAt;
    }

    /**
     * Get finished_at
     *
     * @return datetime 
     */
    public function getFinishedAt()
    {
        return $this->finished_at;
    }

    /**
     * Set root_id
     *
     * @param integer $rootId
     */
    public function setRootId($rootId)
    {
        $this->root_id = $rootId;
    }

    /**
     * Get root_id
     *
     * @return integer 
     */
    public function getRootId()
    {
        return $this->root_id;
    }

    /**
     * Set ordering
     *
     * @param integer $ordering
     */
    public function setOrdering($ordering)
    {
        $this->ordering = $ordering;
    }

    /**
     * Get ordering
     *
     * @return integer 
     */
    public function getOrdering()
    {
        return $this->ordering;
    }

    /**
     * Set priority
     *
     * @param integer $priority
     */
    public function setPriority($priority)
    {
        $this->priority = $priority;
    }

    /**
     * Get priority
     *
     * @return integer 
     */
    public function getPriority()
    {
        return $this->priority;
    }

    /**
     * Set status
     *
     * @param integer $status
     */
    public function setStatus($status)
    {
        $this->status = $status;
    }

    /**
     * Get status
     *
     * @return integer 
     */
    public function getStatus()
    {
        return $this->status;
    }
}