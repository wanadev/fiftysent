<?php
namespace Sb\SendboxBundle\Entity;

use FOS\UserBundle\Entity\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="fos_user")
 */
class User extends BaseUser
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;
    
    /**
     * @ORM\OneToMany(targetEntity="Announce", mappedBy="user", cascade={"remove", "persist"})
     */
    protected $announces;
    
    /**
     * 
     */
    protected $username;
    
    public function __construct()
    {
        parent::__construct();
        // your own logic
    }
    
    /**
     * To String
     *
     * @return string
     */
    public function __toString()
    {
        return $this->username;
    }
    
   

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Add announces
     *
     * @param Sp\SpotoraBundle\Entity\Announce $announces
     */
    public function addAnnounce(\Sp\SpotoraBundle\Entity\Announce $announces)
    {
        $this->announces[] = $announces;
    }

    /**
     * Get announces
     *
     * @return Doctrine\Common\Collections\Collection 
     */
    public function getAnnounces()
    {
        return $this->announces;
    }
}
